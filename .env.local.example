# Copy this file to .env.local and fill in your values.

# Access gate: password guests must enter to view the gallery.
# If unset, any password is accepted (demo only).
NEXT_PUBLIC_ACCESS_PASSWORD="your-secret-password"

# Optional: JWT secret for session signing. Change in production.
JWT_SECRET="your-jwt-secret-change-in-production"

